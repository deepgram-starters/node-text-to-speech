{
  "name": "@deepgram/node-text-to-speech-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "cd frontend && pnpm install",
    "build": "cd frontend && pnpm run build",
    "start": "node --no-deprecation server.js",
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "NODE_ENV=development nodemon --no-deprecation server.js",
    "dev:frontend": "cd frontend && npm run dev -- --port 5173 --no-open --logLevel error"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepgram-starters/node-text-to-speech.git"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.3",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "nodemon": "^3.1.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}